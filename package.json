{
  "name": "SocialApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src/",
    "test": "mocha src/**/*.spec.js",
    "test:watch": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'npm test'",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --i=src/**/*.js --x=src/**/*.spec.js --x=node_modules node_modules/mocha/bin/_mocha -- src/**/*.spec.js",
    "check-coverage": "istanbul check-coverage",
    "updateSchema": "node ./scripts/updateSchema.js"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.2",
    "react": "15.3.1",
    "react-native": "0.32.0",
    "react-relay": "^0.9.2",
    "react-relay-network-layer": "^1.3.3",
    "node-fetch": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-graphql": "^0.2.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-react-native": "^2.0.0",
    "eslint-plugin-sort-class-members": "^1.0.1",
  }
}
